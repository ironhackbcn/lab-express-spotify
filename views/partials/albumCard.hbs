<div class="card">
  {{#if this.images}}
    <div class="card-img-wrapper" style="background-image: url({{this.images.0.url}})"></div>
  {{else}}
  <div class="card-img-wrapper" style="background-image: url(http://www.designshock.com/wp-content/uploads/2016/04/man-4-400.jpg)"></div>
  {{/if}}
  <div class="card-body">
    <h3 class="card-title">{{this.name}}</h3>
    <a href="/tracks/{{this.id}}"><button>View tracks</button></a>

  </div>
</div>