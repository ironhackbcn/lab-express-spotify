<div class="container-tracks">
    <h1>LIST OF TRACKS</h1>
    <div class="titled">
    <H2 class="beguin">TITLE</H2>
    <H2 class="end">LISTEN</H2>
    </div>



    {{#each items}}
    <div class="tracks-box">
        <div class="text-box">
            {{this.name}}
        </div>
        <div class="audio-box">
            <audio controls src="{{this.preview_url}}">Your browser does not support the
                <code>audio</code>element.
            </audio>
        </div>
    </div>
    {{/each}}

</div>